# Vite environment variables for Firebase (copy to .env.local and fill with your values)
# Do NOT commit .env.local with real keys.

VITE_DEMO_MODE=false

# Backend API base URL (leave empty in dev to use Vite proxy; set to your prod URL when building)
# Example: https://agrisense-t12d.onrender.com
VITE_API_BASE=

# Required Firebase config
VITE_FIREBASE_API_KEY=your_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://crop-and-fertilizer-fyp-default-rtdb.asia-southeast1.firebasedatabase.app
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
