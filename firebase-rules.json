{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "sensors": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "results": {
      ".read": "auth != null", 
      ".write": "auth != null"
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
